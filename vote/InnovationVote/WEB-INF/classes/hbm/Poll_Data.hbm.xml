<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
  
<hibernate-mapping package="db.pojo"> 
    <class  name="PollData" table="poll_data"> 
        <id name="PollID"   column="POLL_ID">
        	<generator class="identity"/> 
        </id> 
        <property name="ProjectID" column="PROJECT_ID">  </property> 
        <property name="UserID"    column="USER_ID">     </property> 
        <property name="PollTime"  column="POLL_TIME">   </property> 
        <property name="PollIP"    column="POLL_IP">    </property> 
    </class> 
</hibernate-mapping> 
